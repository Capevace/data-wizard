{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schema.immo/Address",
  "title": "Address",
  "description": "Address of the real estate",
  "type": "object",
  "required": ["street", "number", "city", "postal_code", "country"],
  "properties": {
    "street": {
      "description": "Street",
      "type": "string"
    },
    "number": {
      "description": "House number",
      "type": "string"
    },
    "city": {
      "description": "City",
      "type": "string"
    },
    "postal_code": {
      "description": "Zip code",
      "type": "string"
    },
    "country": {
      "description": "Country",
      "type": "string"
    },
    "latitude": {
      "description": "Latitude",
      "type": "number"
    },
    "longitude": {
      "description": "Longitude",
      "type": "number"
    }
  }
}