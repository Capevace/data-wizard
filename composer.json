{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "AGPL-3.0-only",
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "ext-zip": "*",
        "akaunting/laravel-money": "^5.2",
        "ardagnsrn/ollama-php": "^1.0",
        "blade-ui-kit/blade-heroicons": "^2.3",
        "blade-ui-kit/blade-icons": "^1.6",
        "codewithkyrian/transformers": "^0.5.3",
        "davidhsianturi/blade-bootstrap-icons": "^1.5",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "greghunt/partial-json": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/octane": "^2.5",
        "laravel/prompts": "^0.1.25",
        "laravel/pulse": "^1.2",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.4",
        "novadaemon/filament-pretty-json": "^2.2",
        "nyholm/psr7": "^1.8",
        "openai-php/laravel": "^0.8.1",
        "php-ai/php-ml": "^0.10.0",
        "rubix/ml": "^2.5",
        "spatie/laravel-http-logger": "^1.11",
        "spatie/laravel-medialibrary": "^11.8",
        "spatie/laravel-webhook-server": "^3.8",
        "spatie/pdf-to-image": "^3.0",
        "swaggest/json-schema": "^0.12.42",
        "tempest/highlight": "^2.8",
        "wire-elements/wire-extender": "^0.0.8"
    },
    "require-dev": {
        "buggregator/trap": "^1.10",
        "fakerphp/faker": "^1.9.1",
        "fly-apps/dockerfile-laravel": "^1.0",
        "laradumps/laradumps": "^3.0",
        "laravel/pint": "^1.17",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "openai-php/client": "^0.8.4",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-drift": "^2.5",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Mateffy\\Magic\\": "lib/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "pint"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "codewithkyrian/transformers-libsloader": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "__repositories": [
        {
            "type": "path",
            "url": "../magic-import"
        }
    ]
}
