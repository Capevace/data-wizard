{
	"name": "laravel/laravel",
	"type": "project",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "AGPL-3.0-only",
	"require": {
		"php": "^8.2",
		"ext-dom": "*",
		"ext-fileinfo": "*",
		"ext-simplexml": "*",
		"ext-zip": "*",
		"akaunting/laravel-money": "^5.2",
		"api-platform/graphql": "^4",
		"api-platform/laravel": "^4.1",
		"blade-ui-kit/blade-heroicons": "^2.3",
		"blade-ui-kit/blade-icons": "^1.6",
		"davidhsianturi/blade-bootstrap-icons": "^1.5",
		"filament/filament": "^v3.2",
		"filament/spatie-laravel-media-library-plugin": "^3.2",
		"guzzlehttp/guzzle": "^7.2",
		"laravel/framework": "^11.0",
		"laravel/octane": "^2.5",
		"laravel/prompts": "^0.3",
		"laravel/pulse": "^1.4",
		"laravel/sanctum": "^4.0",
		"laravel/tinker": "^2.8",
		"livewire/livewire": "^3.5",
		"mateffy/color": "^1.0",
		"mateffy/llm-magic": "^1.0.2",
		"novadaemon/filament-pretty-json": "^2.2",
		"nyholm/psr7": "^1.8",
		"spatie/laravel-medialibrary": "^11.8",
		"spatie/laravel-webhook-server": "*",
		"spatie/pdf-to-image": "^3.0",
		"swaggest/json-schema": "^0.12.42",
		"tempest/highlight": "^2.8",
		"wire-elements/wire-extender": "^1.0"
	},
	"require-dev": {
		"buggregator/trap": "^1.11",
		"fakerphp/faker": "^1.23",
		"fly-apps/dockerfile-laravel": "^1.0",
		"laradumps/laradumps": "^3.2",
		"laravel/pint": "^1.18",
		"laravel/sail": "^1.36",
		"mockery/mockery": "^1.4.4",
		"nunomaduro/collision": "^8.4",
		"pestphp/pest": "^2.35",
		"pestphp/pest-plugin-drift": "^2.5",
		"soloterm/solo": "^0.4.0",
		"spatie/laravel-ignition": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/",
            "Mateffy\\FilaTheme\\Shadowy\\": "packages/shadowy-theme/src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi",
			"@php artisan filament:upgrade"
		],
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		],
		"lint": [
			"pint"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "10.x-dev"
		},
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true,
			"php-http/discovery": true,
			"codewithkyrian/transformers-libsloader": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
    "__repositories": [
        {
			"type": "path",
			"url": "../magic/llm-magic"
		}
	]
}
